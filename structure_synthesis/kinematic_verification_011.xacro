      <geometry>
        <mesh filename="/home/nikos/matlab_ws/modular_dynamixel/H54P-200.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 0.9"/>
      </material>
    </visual>
</link>

<!-- START ONLY visual representation of adaptor to lock base in table-->
